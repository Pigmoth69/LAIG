<lsx>

<SCENE>


<INITIALS>
    <frustum near="0.1" far="250" />
    <translation x="0" y="0" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="1.0" sy="1.0" sz="1.0" />
    <reference length="5.0" />
</INITIALS>


<ILUMINATION>
    <ambient r="0.85" g="0.85" b="0.85" a="1.0" />            <!-- global ambient -->
    <doubleside value="0" />                          <!-- double or single side illum. -->
    <background r="0.5" g="0.54" b="0.53" a="1.0" />         <!-- background color -->
</ILUMINATION>


<LIGHTS>
    <LIGHT id="light0">                                    <!-- light identifier -->
        <enable value ="1" />                         <!-- enable/disable -->
        <position x="4" y="8" z="6" w="1" />       <!-- light position -->
        <ambient r="0.5" g="0.5" b="0.5" a="1.0" />        <!-- ambient component -->
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />        <!-- diffuse component -->
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />       <!-- specular component -->
    </LIGHT>
	
	<!-- NOTE: this block "LIGHT" must be repeated as necessary with different "id" -->
	
	<LIGHT id="light1">
        <enable value ="1" />                         
        <position x="10.7" y="8" z="6" w="1" />
        <ambient r="0.5" g="0.5" b="0.5" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />  
        <specular r="1.0" g="1.0" b="1.0" a="1.0" /> 
    </LIGHT>    
</LIGHTS>


<TEXTURES>
    <TEXTURE id="wall">
        <file path="scenes/resources/wall.png" />                             <!-- path to file -->
        <amplif_factor s="10" t="5" />
    </TEXTURE>
	                                                                                                                                                         
	<TEXTURE id="floor">
        <file path="scenes/resources/floor.png" /> 
        <amplif_factor s="1" t="1"/> 
    </TEXTURE>
    
	<TEXTURE id="table">
        <file path="scenes/resources/table.png" /> 	
        <amplif_factor s="1" t="1" /> 
    </TEXTURE>
	
	<TEXTURE id="card1">
        <file path="scenes/resources/card1.png" /> 	
        <amplif_factor s="1" t="1" /> 
    </TEXTURE>
	
	<TEXTURE id="card2">
        <file path="scenes/resources/card2.png" /> 	
        <amplif_factor s="1" t="1" /> 
    </TEXTURE>
	
</TEXTURES>


<MATERIALS>
    <MATERIAL id="fluorescent_white">                                  
        <shininess value="50" />
        <specular r="1" g="1" b="1" a="1" />        <!-- specular reflection -->
        <diffuse r="1" g="1" b="1" a="1" />         <!-- diffuse reflection -->
        <ambient r="1" g="1" b="1" a="1" />         <!-- ambient reflection -->
        <emission r="1" g="1" b="1" a="1" />        <!-- emission component -->
     </MATERIAL>
	 
	<MATERIAL id="white">                                  
        <shininess value="5" />
        <specular r="1" g="1" b="1" a="1" />        <!-- specular reflection -->
        <diffuse r="1" g="1" b="1" a="1" />         <!-- diffuse reflection -->
        <ambient r="1" g="1" b="1" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="0" b="0" a="0" />        <!-- emission component -->
     </MATERIAL>

    <!-- NOTE: the "MATERIAL" block may be repeated as required. Each defined material requires a distinct "id" -->
	
	<MATERIAL id="wood">                                  
        <shininess value="1" />
        <specular r="0.4" g="0.4" b="0.4" a="1" />
        <diffuse r="0.8" g="0.67" b="0.49" a="1" />
        <ambient r="0.8" g="0.67" b="0.49" a="1" />
        <emission r="0" g="0" b="0" a="0" />
     </MATERIAL>
	 
	 <MATERIAL id="metal">                                  
        <shininess value="40" />
        <specular r="0.83" g="0.83" b="0.83" a="1" />
        <diffuse r="0.61" g="0.61" b="0.61" a="1" />
        <ambient r="0.61" g="0.61" b="0.61" a="1" />
        <emission r="0" g="0" b="0" a="0" />
     </MATERIAL>
</MATERIALS>


<LEAVES>
    <!-- next lines define nodes of type leaf; they may be repeated, in any order, as necessary -->
    <LEAF id="default_rect" type="rectangle" args="0 1 1 0" />      <!-- 2D coordinates for left-top and right-bottom vertices. -->
    <LEAF id="top_cylinder" type="cylinder" args="0.5 2 2.5 1 24" />    <!-- height, bottom radius, top radius, sections along height, parts per section -->
    <LEAF id="leg_cylinder" type="cylinder" args="3.5 0.5 0.5 1 24" />
	<!-- <LEAF id="ss" type="sphere" args="ff ii ii" />            radius, parts along radius, parts per section -->
    <!-- <LEAF id="ss" type="triangle" args="ff ff ff  ff ff ff  ff ff ff" />  coordinates of each vertex -->
</LEAVES>


<NODES>

    <ROOT id="cardroom"/>
	
    <NODE id="cardroom">
        <MATERIAL id="null" />      <!-- declared material superimposes the material received from parent node -->
									<!-- id="null" maintains material from parent node        -->
        <TEXTURE id="null" />       <!-- declared texture superimposes the texture received from parent node -->
                                  <!-- id="null" maintains texture from parent node       -->
                                  <!-- id="clear" clears texture declaration received from parent node      -->
        <DESCENDANTS>
            <DESCENDANT id="surfaces" />
			<DESCENDANT id="main_chair" />
			<DESCENDANT id="table" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="surfaces">
        <MATERIAL id="null" />
        <TEXTURE id="null" /> 		
        <DESCENDANTS>
			<DESCENDANT id="floor" />
            <DESCENDANT id="wall_xy" />
			<DESCENDANT id="wall_zy" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="floor">
        <MATERIAL id="wood" />
        <TEXTURE id="floor" /> 
		<TRANSLATION x="0" y="0" z="12" />
		<ROTATION axis="x" angle="-90" />
		<SCALE sx="16" sy="12" sz="1" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="wall_xy">
        <MATERIAL id="white" />
        <TEXTURE id="wall" /> 
        <SCALE sx="16" sy="8" sz="1" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="wall_zy">
        <MATERIAL id="white" />
        <TEXTURE id="wall" /> 
		<TRANSLATION x="0" y="0" z="12" />
		<ROTATION axis="y" angle="90" />
		<SCALE sx="12" sy="8" sz="1" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="main_chair">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
        <DESCENDANTS>
			<DESCENDANT id="chair1" />
			<DESCENDANT id="chair2" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="chair1">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<TRANSLATION x="4" y="0" z="7.5"/>
        <DESCENDANTS>
			<DESCENDANT id="backside" />
			<DESCENDANT id="seat" />
			<DESCENDANT id="leg" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="chair2">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<TRANSLATION x="12" y="0" z="4.5"/>
		<ROTATION axis="y" angle="180" />
        <DESCENDANTS>
			<DESCENDANT id="backside" />
			<DESCENDANT id="seat" />
			<DESCENDANT id="leg" />
        </DESCENDANTS>
    </NODE>
	
	<!-- BACKSIDE CHAIR -->
	
	<NODE id="backside">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
        <DESCENDANTS>
			<DESCENDANT id="back_bs" />
			<DESCENDANT id="top_bs" />
			<DESCENDANT id="front_bs" />
			<DESCENDANT id="right_bs" />
			<DESCENDANT id="left_bs" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="right_bs">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<SCALE sx="0.5" sy="4.5" sz="1" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="left_bs">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<TRANSLATION x="0.5" y="0" z="-3" />
        <SCALE sx="0.5" sy="4.5" sz="1" />
		<ROTATION axis="y" angle="180" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="top_bs">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<TRANSLATION x="0" y="4.5" z="0" />
		<ROTATION axis="x" angle="-90" />
        <SCALE sx="0.5" sy="3" sz="1" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="back_bs">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<TRANSLATION x="0" y="0" z="-3" />
		<ROTATION axis="y" angle="-90" />
        <SCALE sx="3" sy="4.5" sz="0" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="front_bs">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<TRANSLATION x="0.5" y="0" z="0" />
		<ROTATION axis="y" angle="90" />
        <SCALE sx="3" sy="4.5" sz="0" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	
	<!-- SEAT CHAIR -->
	<NODE id="seat">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<ROTATION axis="y" angle="90" />
        <DESCENDANTS>
			<DESCENDANT id="top_seat" />
			<DESCENDANT id="back_seat" />
			<DESCENDANT id="right_seat" />
			<DESCENDANT id="left_seat" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="right_seat">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<TRANSLATION x="0.5" y="1.5" z="0" />
        <SCALE sx="2" sy="0.5" sz="0" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="left_seat">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<TRANSLATION x="2.5" y="1.5" z="-3" />
		<ROTATION axis="y" angle="180" />
        <SCALE sx="2" sy="0.5" sz="0" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="top_seat">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<TRANSLATION x="0.5" y="2" z="0" />
		<ROTATION axis="x" angle="-90" />
        <SCALE sx="2" sy="3" sz="0" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="back_seat">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<TRANSLATION x="0.5" y="1.5" z="-3" />
		<ROTATION axis="x" angle="90" />
        <SCALE sx="2" sy="3" sz="0" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	<!-- LEG CHAIR -->
	<NODE id="leg">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
        <DESCENDANTS>
			<DESCENDANT id="front_leg" />
			<DESCENDANT id="back_leg" />
			<DESCENDANT id="right_leg" />
			<DESCENDANT id="left_leg" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="front_leg">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<TRANSLATION x="2.5" y="0" z="0" />
		<ROTATION axis="y" angle="90" />
        <SCALE sx="3" sy="2" sz="0" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="back_leg">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<TRANSLATION x="2" y="0" z="-3" />
		<ROTATION axis="y" angle="-90" />
        <SCALE sx="3" sy="2" sz="0" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="right_leg">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<TRANSLATION x="2" y="0" z="0" />
        <SCALE sx="0.5" sy="2" sz="0" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="left_leg">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<TRANSLATION x="2.5" y="0" z="-3" />
		<ROTATION axis="y" angle="180" />
        <SCALE sx="0.5" sy="2" sz="0" />
        <DESCENDANTS>
			<DESCENDANT id="default_rect" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="table">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
		<TRANSLATION x="8" y="0" z="6" />
		<ROTATION axis="x" angle="-90" />
        <DESCENDANTS>
			<DESCENDANT id="table_leg" />
			<DESCENDANT id="table_top" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="table_leg">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
        <DESCENDANTS>
			<DESCENDANT id="leg_cylinder" />
        </DESCENDANTS>
    </NODE>
	
	<NODE id="table_top">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
        <DESCENDANTS>
			<DESCENDANT id="top_cylinder" />
        </DESCENDANTS>
    </NODE>

</NODES>


</SCENE>

</lsx>